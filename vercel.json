{
    "rewrites": [
        {
            "source": "/user/:path*",
            "destination": "@API_URL/user/:path*"
        },
        {
            "source": "/board/:path*",
            "destination": "@API_URL/board/:path*"
        },
        {
            "source": "/send_message",
            "destination": "@FLASK_URL/send_message"
        },
        {
            "source": "/webhook",
            "destination": "@FLASK_URL/webhook"
        },
        {
            "source": "/hobbylist",
            "destination": "@FLASK_URL/hobbylist"
        }
    ]
}
